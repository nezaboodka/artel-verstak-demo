
import JS = Artel.JavaScript

external
{
  // Clock

  #JS.ВнешняяРеализация(модуль = "reactronic")
  type Clock = object
  {
    hour: Integer

    minute: Integer

    second: Integer

    ms: Integer

    interval: Integer

    paused: YesNo

    on create(interval: Integer = 1000)

    operation pause(value: YesNo)
  }
}
