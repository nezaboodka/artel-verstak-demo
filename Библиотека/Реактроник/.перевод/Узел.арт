
псевдонимы(английский -> русский)
{
  type Delegate<T>(el, basis) -> Обработчик<Т>(эл, базис)

  type Mode -> Режим
  {
    default -> стандартный
    independent-update -> независимое-обновление
    manual-mount -> ручное-встраивание
  }

  type Priority -> Приоритет
  {
    realtime -> реальный
    normal -> обычный
    background -> фоновый
  }

  type RxNodeDecl -> ДекларацияУзла
  {
    autorun -> автозапуск
    key -> ключ
    mode -> режим
    activation -> активация
    deactivation -> деактивация
    triggers -> триггеры
    preset -> заготовка
  };
}
