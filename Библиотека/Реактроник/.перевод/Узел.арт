
псевдонимы(английский -> русский)
{
  type Delegate<T>(el, basis) -> Обработчик<Т>(эл, базис)

  type Mode -> Режим
  {
    default -> стандартный
    independent-update -> независимое-обновление
    manual-mount -> ручное-встраивание
  }

  type Priority -> Приоритет
  {
    realtime -> реальный
    normal -> обычный
    background -> фоновый
  }

  type RxNodeDecl -> ДекларацияУзла
  {
    on-change -> при-изменении
    key -> ключ
    mode -> режим
    on-create -> при-создании
    on-destroy -> при-уничтожении
    triggers -> триггеры
    preset -> заготовка
  };
}
