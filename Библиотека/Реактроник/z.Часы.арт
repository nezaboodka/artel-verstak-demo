подключить Артель

псевдонимы(русский)
{
  операция транзакция(действие) = transaction(action)

  тип Часы = Clock
  {
    час = hour

    минута = minute

    секунда = second

    мс = ms

    интервал = interval

    на-паузе = paused

    при создании(интервал) = on create(interval)

    операция пауза(значение) = pause(value)
  }

  тип Транзакция = Transaction
  {
    операция сделать(опции, опер) = run(options, func)
  }
}
