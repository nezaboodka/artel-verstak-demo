import Artel
import js = Artel.JavaScript

external
{
  // Worker

  #js.ВнешняяРеализация(модуль = "reactronic")
  type Worker = aspect
  {
    guarded
    {
      id: Integer

      hint: Text

      isCanceled: YesNo

      isFinished: YesNo
    }

    operation cancel(err: Error, restartAfter: Worker? = empty): Object?

    parallel
    operation whenFinished()
  }
}
