    DOCUMENT_POSITION_PRECEDING = ПОЗИЦИЯ_ДОКУМЕНТА_ПРЕДШЕСТВУЮЩАЯ

    DOCUMENT_POSITION_FOLLOWING = ПОЗИЦИЯ_ДОКУМЕНТА_ПОСЛЕДУЮЩАЯ

    DOCUMENT_POSITION_CONTAINS = ПОЗИЦИЯ_ДОКУМЕНТА_СОДЕРЖИТ

    DOCUMENT_POSITION_CONTAINED_BY = ПОЗИЦИЯ_ДОКУМЕНТА_СОДЕРЖИТСЯ_КЕМ

    DOCUMENT_POSITION_IMPLEMENTATION_SPECIFIC = ПОЗИЦИЯ_ДОКУМЕНТА_СПЕЦИФИЧНА_РЕАЛИЗАЦИИ

    prototype = прототип
  }


  type Element = Элемент
  {
    owner-document = документ-владелец

    operation check-visibility(options) = проверить-видимость()

    operation closest(selector) = ближайший(селектор)

    operation get-elements-by-tag-name(qualified-name) =
      получить-элементы-по-имени-тега(квалифицированное-имя)

    operation get-elements-by-tag-name-NS(namespace-URI, local-name) =
      получить-элементы-по-имени-тега-ПИ(адрес-пространства-имён, локальное-имя)

    operation scroll(x, y) = прокрутить(э, ю)

    operation scroll-by(x, y) = прокрутить-на(э, ю)

    operation scroll-to(x, y) = прокрутить-к(э, ю)

    operation add-event-listener(type`, listener, options) =
      добавить-обработчик-события(тип`, обработчик, настройки)

    operation remove-event-listener(type`, listener, options) =
      удалить-обработчик-события(тип`, обработчик, настройки)

    prototype = прототип
  }
