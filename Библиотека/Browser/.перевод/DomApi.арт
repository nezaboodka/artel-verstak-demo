  type EventTarget = ПолучательСобытий
  {
    operation add-event-listener(type`, callback, options) =
      добавить-обработчик-события(тип`, обработчик, настройки)

    operation dispatch-event(event) =
      диспетчировать-событие(событие)

    operation remove-event-listener(type`, callback, options) =
        удалить-собработчик-события(тип`, обработчик, настройки)

    prototype = прототип
  }


  type Node = Узел
  {
    base-URI = базовый-адрес

    child-nodes = подчинённые-узлы

    first-child = первый-подчинённый-узел

    is-connected = подключен

    last-child = последний-подчинённый-узел

    next-sibling = следующий-соседний-узел

    node-name = имя-узла

    node-type = тип-узла

    node-value = значение-узла

    owner-document = собственный-документ

    parent-element = вышестоящий-элемент

    parent-node = вышестоящий-узел

    previous-sibling = предыдущий-соседний-узел

    text-content = текстовое-содержимое

    operation append-child(node) = добавить-подчинённый-узел(узел)

    operation clone-node(deep) = клонировать-узел(глубина)

    operation compare-document-position(other) = сравнить-позицию-документа(другой)

    operation contains(other) = содержит(другой)

    operation get-root-node(options) = получить-корневый-узел(настройки)

    operation has-child-nodes() = имеет-подчинённые-узлы()

    operation insert-before(node, child) = вставить-перед(узел, подчинённый-узел)

    operation is-default-namespace(namespace) = это-основное-пространство-имён(пространство-имён)

    operation is-equal-node(other-node) = это-равный-узел(другой-узел)

    operation is-same-node(other-node) = это-тот-же-узел(другой-узел)

    operation lookup-namespace-URI(prefix) = найти-адрес-пространства-имён(префикс)

    operation lookup-prefix(namespace) = найти-префикс(пространство-имён)

    operation normalize() = нормализовать()

    operation remove-child(child) = удалить-подчинённый-узел(узел)

    operation replace-child(node, child) = заменить-подчинённый-узел(узел, подчинённый-узел)

    ELEMENT_NODE = УЗЕЛ_ЭЛЕМЕНТА

    ATTRIBUTE_NODE = АТРИБУТ_УЗЛА

    TEXT_NODE = ТЕКСТОВЫЙ_УЗЕЛ

    CDATA_SECTION_NODE = CDATA_SECTION_NODE

    ENTITY_REFERENCE_NODE = УЗЕЛ_ССЫЛКИ_НА_СУЩНОСТЬ

