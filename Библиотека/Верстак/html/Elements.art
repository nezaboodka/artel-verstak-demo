operation SectionEx<M = Object?>(
  update: Delegate<El<DOM.HTMLElement, M>>? = none,
  mode: Mode? = none,
  key: Text? = none,
  initialize: Delegate<El<DOM.HTMLElement, M>>? = none,
  finalize: Delegate<El<DOM.HTMLElement, M>>? = none,
  preset: RxNodeDecl<El<DOM.HTMLElement, M>>? = none,
  triggers: Object? = none):
  RxNode<El<DOM.HTMLElement, M>>
{
  result = SectionImpl<M>(`(preset, key, mode, triggers, initialize, update, finalize))
}


