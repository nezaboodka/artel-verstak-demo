import artel
import artel.js

#ВнешняяРеализация(модуль = "reactronic")
external
type Clock = object
{
  hour: Integer

  minute: Integer

  second: Integer

  ms: Integer

  interval: Integer

  paused: YesNo

  on create(interval: Integer = 1000)

  operation pause(value: YesNo)
}

#ВнешняяРеализация(модуль = "reactronic")
external
type Transaction = object
{
  global
  operation run<T>(options: Object? = empty, func: operation(): T): T
}
